# Ignore everything under the data dir by default
aid_dashboard_data/**

# Re-allow the data dir itself and all subdirs (so negated file rules can match)
!aid_dashboard_data/
!aid_dashboard_data/**/

# Keep only lightweight outputs
!aid_dashboard_data/**/*.geojson
!aid_dashboard_data/**/*.json

# Still ignore heavy/raw stuff
aid_dashboard_data/**/raw/**
aid_dashboard_data/**/tmp_shapefile/**
aid_dashboard_data/**/*.shp
aid_dashboard_data/**/*.dbf
aid_dashboard_data/**/*.shx
aid_dashboard_data/**/*.prj
aid_dashboard_data/**/*.qpj
aid_dashboard_data/**/*.zip